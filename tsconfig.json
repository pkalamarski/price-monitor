{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": ".",
    "skipLibCheck": true,
  },
  "exclude": ["node_modules"]
}