{
  "compilerOptions": {
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "outDir": "dist",
    "lib": ["es2019", "DOM"]
  },
  "exclude": ["node_modules"]
}
